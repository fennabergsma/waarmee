@article{wellwood2012,
  title = {Measuring and {{Comparing Individuals}} and {{Events}}},
  volume = {29},
  doi = {10.1093/jos/ffr006},
  abstract = {This squib investigates parallels between nominal and verbal comparatives. Building on key insights of Hackl (2000) and Bale \& Barner (2009), we show that more behaves uniformly when it combines with nominal and verbal predicates: (i) it cannot combine with singular count NPs or perfective telic VPs; (ii) grammatical properties of the predicates determine the scale of comparison\textemdash{}plural marked NPs and habitual VPs are compared on a scale of cardinality, whereas mass NPs and perfective (atelic) VPs are (often) compared along non-cardinal, though monotonic, scales. Taken together, our findings confirm and strengthen parallels that have independently been drawn between the nominal and verbal domains. In addition, our discussion and data, drawn from English, Spanish, and Bulgarian, suggest that the semantic contribution of more can be given a uniform analysis.},
  number = {2},
  journal = {Journal of Semantics},
  author = {Wellwood, Alexis and Hacquard, Valentine and Pancheva, Roumyana},
  year = {2012},
  pages = {207-228}
}

