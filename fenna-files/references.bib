@article{harris2006,
  title = {Revisiting {{Anaphoric Islands}}},
  volume = {82},
  abstract = {Postal (1969) discusses words as anaphoric islands. So-called OUTBOUND ANAPHORA was further discussed in a series of papers published in the 1970s through the early 1990s, but INBOUND ANAPHORA, such as Postal's *himite (beside McCarthyite), has received less attention. It is shown here that a wide variety of words in Georgian are based on pronouns, including fully referential personal pronouns, reflexive pronouns, question words, quantifiers, and negative pronouns. Thus, the nonoccuring combinations of English are a language-particular problem.},
