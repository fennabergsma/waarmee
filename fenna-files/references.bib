@article{iwata1996,
  title = {Motion and {{Extent}}: {{Two Sides}} of the {{Same Coin}}},
  volume = {50},
  abstract = {This paper examines the relationship between a motion sentence (e.g., Amy went from Denver to Indianapolis.) and an extent sentence (e.g., The road goes from Denver to Indianapolis.), building upon Jackendoff's (1983) analysis. It is shown that the semantic functions for motion and extent senses, respectively GO and GOext, are distinct, but that they share a great deal of internal structure. While both functions express successive transition from one component state to the next, they differ in (a) the presence vs. absence of the passage of time, and (b) the identity of the mover through transition. The analysis presented here based on semantic functions comes closer to an overall characterization of the motion/extent contrast than the image-schema-based approach (Lakoff 1987) or the metaphor-based approach (Lakoff \& Turner 1989). It can also be easily extended to account for types of extent sentences that the literature has not focussed upon.},
