@article{han2004a,
  title = {Disjunction, {{Focus}}, and {{Scope}}},
  volume = {35},
  abstract = {This article presents the observation that disjunction cannot take wide scope in negative non-$\backslash$emphwh-questions and declaratives with a preposed negative element. This rules out the alternative question reading for non-$\backslash$emphwh-questions with preposed negation and the wide scope $\backslash$emphor reading for neg-inverted declaratives. We show that effects parallel to the ones associated with preposed negation can be reproduced in affirmative non-$\backslash$emphwh-questions and declaratives when focus is involved. We propose that preposed negation in non-$\backslash$emphwh-questions and preposed negative adverbials in declaratives necessarily contribute focus marking (in particular, verum focus) and argue that the lack of wide scope disjunction reading in both declaratives and non-$\backslash$emphwh-questions results as a by-product of the interaction between focus and the LF syntax of disjunctive structures, which we argue involves ellipsis.},
