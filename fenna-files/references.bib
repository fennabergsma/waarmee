@article{bejar1999,
  title = {Multiple {{Case Checking}}},
  volume = {2},
  abstract = {This paper exami nes Multiple Case Checking (MCC) in raising constructions in light of minimalist Case theory and an interpretive view of vocabulary insertion. In general, Case theory excludes the option of a chain receiving more than one Case. However, certain constructions arguably demonstrate that this is possible. We present a range of MCC phenomena, showing that they are incompatible with early insertion models. Inherent Case examples, such as in Icelandic, are accounted for by markedness. For structural Case examples we present an analysis in which both Case assignment and Case checking are utilized. We account for aspects of English, Norwegian, and Niuean MCC using two parameters: whether or not Case features move along with NPs, and whether or not PF accesses whole chains.},
