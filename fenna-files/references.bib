@article{krifka2010,
  title = {$\backslash${{emphBefore}} and $\backslash${{emphAfter}} without {{Coercion}}: {{Comment}} on the {{Paper}} by {{Cleo Condoravdi}}},
  volume = {28},
  abstract = {The following contribution was inspired by Cleo Condoravdi's article on NPI licensing in temporal clauses (Condoravdi 2010). Condoravdi gives a coherent and comprehensive account of before which crucially involves coercion of proposi- tions to the earliest or maximal times at which the propositions are true, and a modal component for non-factual interpretations. I argue for a non-modal, non-coercive analysis of clauses like [A before B] as `A is the case when B has not been the case', triggering a conversational implicature that B will be the case later. I will also discuss temporal operators involving measure phrases, like three hours before.},
