@article{harley2004,
  title = {Wanting, {{Having}}, and {{Getting}}: {{A Note}} on {{Fodor}} and {{Lepore}} 1998},
  volume = {35},
  abstract = {This article takes up Fodor and Lepore's (1998) account of the meaning of [$\backslash$emphwant DP] structures, according to which the verb $\backslash$emphto have is introduced at interpretation. With certain DP complements a $\backslash$emphwant to have DP paraphrase of $\backslash$emphwant DP is ill formed; the correct paraphrase uses $\backslash$emphget or $\backslash$emphgive. To allow for this, Fodor and Lepore would have to vary the introduced verb depending on the meaning of the DP, but this would make their proposal ``co-compositional,'' defeating its original purpose. If $\backslash$emphhave, $\backslash$emphget, and $\backslash$emphgive all contain the abstract preposition P\_have (Harley 1995, Richards 2001), however, Fodor and Lepore's treatment may be appropriately revised: the element introduced by $\backslash$emphwant is not $\backslash$emphhave but P\_have. Fodor and Lepore can avoid co-compositionality at the price of allowing lexical decomposition.},
  number = {2},
