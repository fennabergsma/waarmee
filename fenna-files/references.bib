@article{hazout2007,
  title = {On the {{Rleation}} between {{Expletive}} $\backslash${{emphThere}} and {{Its Associate}}: {{A Reply}} to {{Williams}}},
  volume = {39},
  abstract = {The relation between expletive there and its associate involves agreement, as suggested in Hazout 2004, and not theta-role assignment, as suggested in Williams 1994, 2006. This difference reflects radically different assumptions about the nature of the subject-predicate relation. The analysis in Hazout 2004 provides a superior account of the empirical facts and affords insights that are missed by the account in Williams 1994.},
