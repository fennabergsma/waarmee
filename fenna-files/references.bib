@article{hazout2004,
  title = {The {{Syntax}} of {{Existential Constructions}}},
  volume = {35},
  abstract = {This article outlines an account of the syntax of existential constructions (e.g., $\backslash$emphThere are too many problems) based on a view of the postcopular NP as a predicate. This NP figures as the predicate in an embedded clausal complement of $\backslash$emphbe with expletive $\backslash$emphthere as its subject. $\backslash$emphThere consequently moves to the higher Spec, IP position for Case-theoretic reasons. Existential constructions and existential interpretation are a particular instance of a wider phenomenon involving the use of predicates of various categories with expletive subjects (e.g., $\backslash$emphIt is cold). Long-distance agreement between the main (inflected) verb and the postcopluar NP is a combined effect of the relation of subject-predicate agreement holding between the expletive subject ($\backslash$emphthere) and a predicate NP within the embedded clausal structure and the relation of specifier-head agreement (feature checking) bewteen the arised expletive and the matrix I/T. This analysis is generalized to other cases of long-distance agreement (e.g., $\backslash$emphThere appeared a man). It is shown that an analysis based on teh notion $\backslash$emphAgree (Chomsky 2000) is empirically inadequate. Well-known restrictions on the distribution of NPs/DPs in existential constructions follow from the proposed analysis.},
  number = {3},
  journal = {Linguistic Inquiry},
  author = {Hazout, Ilan},
  year = {2004},
  pages = {393-430}
}

