@article{cowper1998,
  title = {The {{Simple Present Tense}} in {{English}}: {{A Unified Treatment}}},
  volume = {52},
  abstract = {This paper provides a unified account of the matrix uses of the English simple present. The differences between eventive and stative sentences in the simple present, as well as reportive, futurate and habitual uses of eventive sentences are shown to derive straightforwardly from a single constraint on grammatical tense systems: the Principle of Non-Simultaneity of Points. The analysis supports a view of speech time as a moment, rather than an interval, in the unmarked case, and also supports the purely Davidsonian view that only eventive sentences contain an event-denoting element.},
  number = {1},
  journal = {Studia Linguistica},
  author = {Cowper, Elizabeth},
  year = {1998},
  pages = {1-18}
}

@article{cowper2005a,
  title = {The {{Geometry}} of {{Interpretable Features}}: {{Infl}} in {{English}} and {{Spanish}}},
  volume = {81},
  abstract = {This article proposes a feature-geometric analysis of the interpretable features of Infl, using MINIMALIST syntax and DISTRIBUTED MORPHOLOGY. A small universal set of monovalent interpretable features and a set of entailment relations among them provide the basis for a principled account of the tense systems of English and Spanish. While each feature, each lexical item, and each vocabulary item has a unified representation, surface polysemy is shown to arise from the mappings between them. Crosslinguistic variation is shown to arise from the different features chosen by each language and from the ways in which each language assembles its features into lexical items and vocabulary items. In addition, the presence or absence of a dependent feature F in a given language is shown to have important consequences for the semantic interpretation of the feature dominating F. These three possible differences interact to produce the significant superficial differences between the tense systems of the two languages.},
  number = {1},
  journal = {Language},
  author = {Cowper, Elizabeth},
  year = {2005},
  pages = {10-46}
}

