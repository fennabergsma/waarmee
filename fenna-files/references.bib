@article{ting2003,
  title = {The {{Nature}} of the {{Particle}} $\backslash$emphsuo in {{Mandarin Chinese}}},
  volume = {12},
  abstract = {This article argues for an analysis of Mandarin chinese $\backslash$emphsuo as a resumptive pronominal clitic: $\backslash$emphsuo is suggested to be the head of a nominal projection, licensed by being bound by a null operator and adjoined to Io via head movement. This analysis will be shown to account for the various properties of $\backslash$emphsuo, including its surface ofer with respect to other elements in the clause, the complementary distribution between $\backslash$emphsuo and the resumptive pronoun $\backslash$emphta 'he', the "climbing" phenomenon, and the licesning asymmetry with respect to $\backslash$emphsuo's distribution, namely, the fact that $\backslash$emphsuo is allowed in locative inversion constructions but not in relativization of subject, manner and reason expressions. This proposed analysis will be compared with that of Chiu (1995) and will conclude that the facts of $\backslash$emphsuo do not support the positing of an agreement-like projeciton in Chinese.},
