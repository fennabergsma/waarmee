@article{kennedy2008,
  title = {Evidence for {{Morphoprosodic Alignment}} in {{Reduplication}}},
  volume = {39},
  abstract = {Morphoprosodic Alignment (MPA) is a nontemplatic model of reduplication designed to account for languages with multiple reduplicative subpatterns. The premise of MPA is that reduplicative morphemes can be stem-internal or stem-external and that this distinction is visible to the phonological component through general constraints on the association of stem-internal and stem-external morphemes to prosodic categories. I illustrate the model with Moronene, Klamath, and Gooniyandi, each of which has several reduplicative morphemes. MPA meets the challenge for an optimality-theoretic model to account for such systems without resorting to morpheme-specific indexed constraints or cophonological constraint hierarchies.},
