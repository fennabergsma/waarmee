@article{kleemann-kramer2010,
  title = {On {{Apparent NP}}-{{Internal Focus Particles}} in {{German}}},
  volume = {13},
  abstract = {It is well known that the distribution of focus particles such as only, also and even varies across languages. Focus particle placement is less restricted, for example, in English, which features adverbial as well as adnominal focus particles. That is opposed to German which only has adverbial focus particles. According to B\"uring and Hartmann's (Nat Lang Linguist Theory 19:229\textendash{}281, 2001) Particle Theory, German focus particles only adjoin to nodes which are non-arguments. The correctness of the Particle Theory is not uncontroversial. It has been argued that the Particle Theory makes the wrong predictions about focus particle placement within NPs. The aim of this paper is to show that apparent counterexamples, in fact, provide new evidence in favor of B\"uring and Hartmann's Particle Theory. It argues that NP-internal focus particles adjoin to extra non-nominal functional projections which do count as focus particle adjunction sites according to the Particle Theory. Evidence comes from German as well as Dutch NP-internal focus particles.},
  number = {1},
