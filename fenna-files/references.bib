@article{ha2004,
  title = {The {{Existential Reading}} of {{WH}}-{{Words}} and {{Their Scope Relations}}},
  volume = {40},
  abstract = {WH-words in Korean can have either a question word or an existential indefinite interpretation. The WH-subject Nwukwu who in can be ambiguous between a WH-Question reading and a WH-indefinite reading. The WH-word can also be used in a declarative sentence as a WH-indefinite. Certain previous literature (Chung 1996; Jang and Kim 1996; Jang 1999) has distinguished the WH-Question reading from the WH-indefinite reading in terms of an indirect question analysis. Under this view, when the WH-word is bound by a Q-operator, then it has the WH-Question reading. However, the view that the indefinite reading arises from an embedded question has some empirical problems. In this paper, I will show that the WH-compound form (WH+be+Q) has different scope-taking relations from the bare WH-existential form (WH-\O{}). And I will explain this scopal difference using a choice function mechanism: namely, the Q-particle in WH+Qparticle forms introduces a choice function to WH-words.},
  number = {1},
  journal = {Proceedings from the Annual Meeting of the Chicago Linguistic Society},
  author = {Ha, Seungwan},
  year = {2004},
  pages = {83-95}
}

