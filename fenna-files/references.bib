@article{han2007b,
  title = {V-{{Raising}} and {{Grammar Competition}} in {{Korean}}: {{Evidence}} from {{Negation}} and {{Quantifier Scope}}},
  volume = {38},
  abstract = {In a head-final language, V-raising is hard to detect since there is no evidence from the string to support a raising analysis. If the language has a cliticlike negation that associates with the verb in syntax, then scope facts concerning negation and a quantified object NP could provide evidence regarding the height of the verb. Even so, such facts are rare, especially in the input to children, and so we might expect that not all speakers exposed to a head-final language acquire the same grammar as far as V-raising is concerned. Here, we present evidence supporting this expectation. Using experimental data concerning the scope of quantified NPs and negation in Korean, elicited from both adults and 4-year-old children, we show that there are two populations of Korean speakers: one with V-raising and one without.},
