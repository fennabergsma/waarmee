@article{dehe2009,
  title = {On the {{Prosody}} and {{Syntax}} of {{DPs}}: {{Evidence}} from {{Italian Noun Adjective Sequences}}},
  volume = {27},
  abstract = {This study tests a syntactic property\textemdash{}namely the availability of N- vs. NP-raising in DPs\textemdash{}through prosodic means. The opposition between N- and NP-raising is central to the ongoing debate about the internal representation of DPs, yet it often eludes testing by syntactic means alone. As we show in this study, the two syntactic hypotheses are instead neatly distinguished by the distinct prosodic phrasing predicted by each operation. In this paper, we present the results of an empirical experiment designed to test the prosodic phrasing of N-A and A-N sequences in Italian and the corresponding syntactic implications. As prosodic cues, we use syllabic and word lengthening effects induced by phonological phrase boundaries. According to our results, A and N share the same phonological phrase in both orders. Regarding the syntactic implications of this finding, we show that under all current models of syntax-prosody mapping the underlying syntactic structure responsible for the attested prosodic phrasing must necessarily rely on N-raising. Finally, we propose an analysis of Italian DPs where the N-raising operation found necessary in light of the attested prosodic phrasing is reconciled with the evidence for DP-internal phrasal movement discussed in Cinque (2005 2006).},
  number = {1},
  journal = {Natural Language and Linguistic Theory},
  author = {Deh\'e, Nicole and {Samek-Lodovici}, Vieri},
  year = {2009},
  pages = {45-75}
}

@article{dehe2005,
  title = {The {{Optimal Placement}} of $\backslash$emphup and $\backslash$emphab \textendash{} a {{Comparison}}},
  volume = {8},
  abstract = {The $\backslash$emphparticle verb construction (PVC), also referred to in the literature as $\backslash$emphphrasal verbs or $\backslash$emphseparable complex verb, occurs in most if not all of the Germanic languages. The work presented here deals with a comparison of the transitive PVC in English and German. In English, the construction occurs in two alternating word orders ($\backslash$emphThey called off the concert vs. $\backslash$emphThey called the concert off). In German, on the other hand, only one order is possible ($\backslash$emphSie sagten das Konzert ab vs. $\backslash$emph*She sagten ab das Konzert; *Se absagten das Konsert). The central question is why this kind of word order alternation is possible in a language with otherwise relatively strict word order, such as English, but not in a related langauge such as German, which is otherwise freer in its constituent ordering, allowing, e.g., for scrambling. In this article, the pattern is explained in terms of (the raking of) violable universal constraints from different modules of grammar. I introduce a PVC-related syntactic constraint which punsihes particle pied-piping. I argue that it is the rank of this constraint with respect to a nmber of prosodic constraints that is responsible for the variation between English and German.},
  number = {3},
  journal = {The Journal of Comparative Germanic Linguistics},
  author = {Deh\'e, Nicole},
  year = {2005},
  pages = {185-224}
}

