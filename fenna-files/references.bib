@article{julien2002,
  title = {Optional $\backslash$emphha in {{Swedish}} and {{Norwegian}}},
  volume = {5},
  abstract = {In Norwegian and Swedish, the auxiliary $\backslash$emphha 'have' can somtimes be omitted. Earlier analyses have treated this as a unitary phenomenon. However, while $\backslash$emphha can only be left out after modal verbs in Norwegian, $\backslash$emphha-omission has a wider distribution in Swedish. This indicates that the optionality of $\backslash$emphha depends on more than one factor. In constructions with modal verbs, hte head that is spelled out as $\backslash$emphha can be present or absent in both languages. That is, the optionality of $\backslash$emphha is connected to the optionality of generating the relevant head. In Swedish, there is the additional possibility of generating $\backslash$emphha without spelling it out. More precisely, $\backslash$emphha need not be spelled out if $\backslash$emphha shares its features with some element that is overtly realized. Still, ther eis much variation between speakers of Swedish concerning the distribution of optional $\backslash$emphha. It appears that a number of grammatical features contribute to the optionality of a phonologically realized $\backslash$emphha.},
  number = {1},
