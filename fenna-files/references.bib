@article{boeckx2006c,
  title = {Honorification as {{Agreement}}},
  volume = {24},
  abstract = {Bobaljik and Yatsushiro (this volume) take issue with Boeckx and Niinuma's (2004) claim that object honorification in Japanese can (and should) be assimilated to more familiar instances of (verb\textendash{}object) agreement. I show that Bobaljik and Yatsushiro's criticism is too weak to abandon the kind of analysis Boeckx and Niinuma proposed. First, Bobaljik and Yatsushiro offer no alternative analysis. I claim that in the absence of a full-fledged alternative conception of honorification, Boeckx and Niinuma's kind of analysis is the null hypothesis. Second, the details they take issue with are not nearly as lethal to Boeckx and Niinuma's original analysis as Bobaljik and Yatsushiro suggest.},
