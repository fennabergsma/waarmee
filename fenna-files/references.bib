@article{davis2001,
  title = {Arabic {{Hypocoristics}} and the {{Status}} of the {{Consonantal Root}}},
  volume = {32},
  abstract = {There is currently a controversy regarding the lexical (morphemic) status of hte consonantal root in teh Semitic languages. Bat-El (1994) and Ratcliffe (1997) have argued against the lexical status of the consonantal root in Hebrew and Arabic, respectively. However, Prunet, B\'eland, and Idrissi (2000) present Arabic aphasic evidence supporting the lexical (morphemic) status of the consonantal root for Arabic. In this article we offer supporting evidence from Arabic hypocoristics for the morphemic status of the consonantal root. WE argue that hypocoristic formation is an output-to-output word formation process that nonetheless references the consonantal root. We then discuss implications.},
