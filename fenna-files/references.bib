@article{carstairs-mccarthy2008,
  title = {System-{{Congruity}} and {{Violable Constraints}} in {{German Weak Declension}}},
  volume = {26},
  abstract = {Among the patterns of declension exhibited by German nouns and adjectives, there are some that are traditionally labelled `weak'. It is argued here that the behaviour of `weak' noun and adjective forms can be best understood if their inflectional suffixes are regarded not as expressing morphosyntactic properties such as gender and case but rather as the outcome of conflicting ranked constraints governing what an optimal noun or adjective should look like in different contexts. For example, an attributive adjective should carry a suffix; a nominative singular form should carry no suffix; and the default inflectional affix is -en. These language-particular constraints reflect some of the `system-defining structural properties' attributed to German by the late Wolfgang Ullrich Wurzel.},
