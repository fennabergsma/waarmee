}

@article{panagiotidis2003a,
  title = {$\backslash${{emphOne}}, {{Empty Nouns}} and \$\th{}eta\$-{{Assignment}}},
  volume = {34},
  abstract = {The standard analysis that the "pronominal count nout" $\backslash$emphone is an N'- or NP-level element is challenged and it is argued to be an N0. Moreover, the behavior of $\backslash$emphone is identified with that of a phonologically empty counterpart. The fact that these N heads lack descriptive content is shown to be the source of two of their distinctive properties: their inability to take arguments, which accounts for their superficially phrasal status, and their triggering of pronominal reference. The existence of a [pronominal] feature is argued agains; instead, such noun heads' lack of descriptive content is claimed to be what LF interprets as "pronominal."},
