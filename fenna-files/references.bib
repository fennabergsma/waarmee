@article{schwarz2005,
  title = {Scalar {{Additive Particles}} in {{Negative Contexts}}},
  volume = {13},
  abstract = {German has a family of expressions corresponding to the English scalar additive particle even, featuring the negative polarity items einmal and auch nur in addition to affirmative sogar. This study reports novel findings on the meanings of einmal and auch nur which are unexpected under existing analyses of English even. First, the particles einmal and auch nur are shown to differ from sogar in that they consistently contribute information about the truth values of alternative propositions, as opposed to their mere likelihood. Second, it is shown that the implications in question do not have the compositional behavior of presuppositions and instead call for the assumption that einmal and auch nur quantify existentially over alternative propositions at the level of truth conditions.},
